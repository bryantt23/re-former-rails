<%= form_with model: @user, url: "/users" do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %><br><br>

    <%= f.label :email %>
    <%= f.text_field :email %><br><br>
    
    <%= f.label :password %>
    <%= f.password_field :password %><br><br>
    <%= f.submit "Submit" %>
<% end %>